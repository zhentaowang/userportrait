<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.adatafun.userportrait.mapper.DimGenerationMapper">
  <resultMap id="BaseResultMap" type="com.adatafun.userportrait.model.DimGeneration">
    <id column="id" jdbcType="INTEGER" property="id" />
    <result column="name" jdbcType="VARCHAR" property="name" />
    <result column="age" jdbcType="BIGINT" property="age" />
  </resultMap>
  <sql id="Base_Column_List">
    id, name, age
  </sql>
  <select id="getGenerationList" parameterType="map" resultType="long">
    select age from dim_generation where id in (${ids}) order by age asc
  </select>
</mapper>